{
    "name": "lzpeng/hyperf-auth-guard",
    "description": "authentication library for hyperf",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lzpeng\\HyperfAuthGuard\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "authors": [
        {
            "name": "liuzhanpeng",
            "email": "liuzhanpeng@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "hyperf/session": "^3.1",
        "hyperf/di": "^3.1",
        "hyperf/framework": "^3.1",
        "hyperf/http-server": "^3.1",
        "hyperf/db-connection": "^3.1",
        "hyperf/cache": "^3.1",
        "symfony/event-dispatcher": "^6.4",
        "hyperf/redis": "^3.1"
    },
    "extra": {
        "hyperf": {
            "config": "Lzpeng\\HyperfAuthGuard\\ConfigProvider"
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.36",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
